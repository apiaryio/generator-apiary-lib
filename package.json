{
  "name": "generator-apiary-lib",
  "version": "0.0.1",
  "description": "Apiary Library Generator",
  "bin": {
    "apiary-lib": "./apiary-lib.sh"
  },
  "scripts": {
    "lint": "eslint app --ext .es6",
    "precompile": "npm run lint",
    "compile": "babel app -x .es6 --out-dir app",
    "pretest": "npm run compile",
    "test": "mocha --compilers es6:babel/register -R spec --recursive",
    "prepublish": "npm run compile",
    "update-coffeelint": "curl 'https://raw.githubusercontent.com/apiaryio/coffeescript-style-guide/master/coffeelint.json' > app/templates/coffee/coffeelint.json"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:apiaryio/generator-apiary-lib.git"
  },
  "author": "Apiary Czech Republic, s.r.o. <support@apiary.io>",
  "license": "MIT",
  "files": [
    "app",
    "apiary-lib.sh"
  ],
  "keywords": [
    "yeoman-generator"
  ],
  "dependencies": {
    "deasync": "^0.1.3",
    "glob": "^5.0.15",
    "lodash": "^3.10.1",
    "underscore.string": "^3.2.2",
    "yeoman-generator": "^0.21.1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "chai": "^3.4.0",
    "eslint": "^1.7.3",
    "eslint-config-airbnb": "^0.1.0",
    "mocha": "^2.3.3"
  },
  "peerDependencies": {
    "yo": "^1.4.8"
  },
  "preferGlobal": true
}
